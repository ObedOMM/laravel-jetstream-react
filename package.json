{
  "name": "laravel-jetstream-react",
  "version": "0.2.1",
  "author": "Ozzie Neher @ozziexsh",
  "bin": {
    "laravel-jetstream-react": "./bin/run"
  },
  "bugs": "https://github.com/TravoShare/laravel-jetstream-react-installer/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "cli-ux": "^5.6.3",
    "fs-extra": "^10.0.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^12.0.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/TravoShare/laravel-jetstream-react-installer",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "laravel-jetstream-react",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "TravoShare/laravel-jetstream-react-installer",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && cp -R src/stubs lib/stubs && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabs": false,
    "tabWidth": 2,
    "trailingComma": "all",
    "printWidth": 80,
    "arrowParens": "avoid"
  }
}
